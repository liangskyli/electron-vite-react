{
  "name": "electron-vite-react",
  "version": "1.0.0",
  "description": "一套基于electron、react19、ts、vite6的项目模板,帮助快速搭建react项目",
  "keywords": [
    "electron",
    "react19 ts template",
    "react template",
    "react",
    "template",
    "vite"
  ],
  "homepage": "https://github.com/liangskyli/electron-vite-react",
  "bugs": "https://github.com/liangskyli/electron-vite-react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/liangskyli/electron-vite-react"
  },
  "license": "MIT",
  "author": "liangsky <640634387@qq.com>",
  "type": "module",
  "main": "./dist-electron/main/index.js",
  "scripts": {
    "build:dev": "pnpm electron-vite build -m dev",
    "build:pro": "pnpm electron-vite build -m pro",
    "build:pro:mac": "npm run build:pro && electron-builder --mac",
    "build:pro:win": "npm run build:pro && electron-builder --win",
    "build:test": "pnpm electron-vite build -m test",
    "coverage": "vitest run --coverage",
    "deps:check": "pnpm outdated -r",
    "dev": "electron-vite -m dev",
    "dev:mock": "electron-vite -m dev-mock",
    "dev:mock-server": "pnpm mock-server -c mock.config.ts",
    "dev:pro": "electron-vite -m pro",
    "dev:test": "electron-vite -m test",
    "preinstall": "npx only-allow pnpm",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:style && pnpm run lint:format",
    "lint:eslint": "eslint --max-warnings 0  \"**/*.{ts,tsx,js,jsx,cjs,mjs}\" --fix",
    "lint:format": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,html,less,scss,css,json}\"",
    "lint:style": "stylelint \"**/*.{css,less,scss}\" --fix",
    "prepare": "husky",
    "preview:dev": "pnpm electron-vite preview -m dev",
    "preview:pro": "pnpm electron-vite preview -m pro",
    "preview:test": "pnpm electron-vite preview -m test",
    "test": "vitest run",
    "test-u": "vitest -u run",
    "typecheck": "tsc --noEmit",
    "update:deps": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "electron-log": "^5.3.2",
    "koffi": "^2.10.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/prompt-cli": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@liangskyli/axios-request": "^0.1.1",
    "@liangskyli/mock": "^5.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^3.0.8",
    "axios": "^1.8.2",
    "classnames": "^2.5.1",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "immer": "^10.1.1",
    "jsdom": "^26.0.0",
    "less": "^4.2.2",
    "lint-staged": "^15.4.3",
    "optics-ts": "^2.4.1",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "react": "^19.0.0",
    "react-dev-inspector": "^2.0.1",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0",
    "sass": "^1.85.0",
    "stylelint": "^16.15.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.1",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.0.8",
    "zustand": "^5.0.3"
  },
  "packageManager": "pnpm@9.15.5",
  "engines": {
    "node": "^18.20.0 || ^20.0.0 || >=22.0.0",
    "pnpm": ">=9"
  }
}
