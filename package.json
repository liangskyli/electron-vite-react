{
  "name": "electron-vite-react",
  "version": "1.0.0",
  "description": "一套基于electron、react19、ts、vite6的项目模板,帮助快速搭建react项目",
  "keywords": [
    "electron",
    "react19 ts template",
    "react template",
    "react",
    "template",
    "vite"
  ],
  "homepage": "https://github.com/liangskyli/electron-vite-react",
  "bugs": "https://github.com/liangskyli/electron-vite-react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/liangskyli/electron-vite-react"
  },
  "license": "MIT",
  "author": "liangsky <640634387@qq.com>",
  "type": "module",
  "main": "./dist-electron/main/index.cjs",
  "scripts": {
    "build:dev": "pnpm electron-vite build -m dev",
    "build:pro": "pnpm electron-vite build -m pro",
    "build:pro:mac": "npm run build:pro && electron-builder --mac",
    "build:pro:win": "npm run build:pro && electron-builder --win",
    "build:test": "pnpm electron-vite build -m test",
    "coverage": "vitest run --coverage",
    "deps:check": "pnpm outdated -r",
    "dev": "electron-vite -m dev",
    "dev:mock": "electron-vite -m dev-mock",
    "dev:mock-server": "pnpm mock-server -c mock.config.ts",
    "dev:mock:web": "vite -m dev-mock",
    "dev:pro": "electron-vite -m pro",
    "dev:test": "electron-vite -m test",
    "preinstall": "npx only-allow pnpm",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:style && pnpm run lint:format",
    "lint:eslint": "eslint --max-warnings 0  \"**/*.{ts,tsx,js,jsx,cjs,mjs}\" --fix",
    "lint:format": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,html,less,scss,css,json}\"",
    "lint:style": "stylelint \"**/*.{css,less,scss}\" --fix",
    "prepare": "husky",
    "preview:dev": "pnpm electron-vite preview -m dev",
    "preview:pro": "pnpm electron-vite preview -m pro",
    "preview:test": "pnpm electron-vite preview -m test",
    "test": "vitest run",
    "test-u": "vitest -u run",
    "typecheck": "tsc --noEmit",
    "update:deps": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "electron-log": "^5.4.2",
    "electron-updater": "^6.6.2",
    "koffi": "^2.13.0"
  },
  "devDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/prompt-cli": "^19.8.1",
    "@eslint/js": "^9.33.0",
    "@liangskyli/axios-request": "^0.2.0",
    "@liangskyli/mock": "^6.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^3.2.4",
    "antd": "^5.27.0",
    "axios": "^1.11.0",
    "classnames": "^2.5.1",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "immer": "^10.1.1",
    "jsdom": "^26.1.0",
    "less": "^4.4.0",
    "lint-staged": "^16.1.5",
    "optics-ts": "^2.4.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.1.1",
    "react-dev-inspector": "^2.0.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.0",
    "sass": "^1.90.0",
    "stylelint": "^16.23.1",
    "stylelint-config-recess-order": "^7.2.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.2.6",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4",
    "zustand": "^5.0.7"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": "^20.19.0 || >=22.0.0",
    "pnpm": ">=10"
  }
}
