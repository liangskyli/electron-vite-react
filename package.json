{
  "name": "electron-vite-react",
  "version": "1.0.0",
  "description": "一套基于electron、react、ts、vite的项目模板,帮助快速搭建react项目",
  "keywords": [
    "electron",
    "react ts template",
    "react template",
    "react",
    "template",
    "vite"
  ],
  "homepage": "https://github.com/liangskyli/electron-vite-react",
  "bugs": "https://github.com/liangskyli/electron-vite-react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/liangskyli/electron-vite-react"
  },
  "license": "MIT",
  "author": "liangsky <640634387@qq.com>",
  "type": "module",
  "main": "./dist-electron/main/index.cjs",
  "scripts": {
    "build:dev": "pnpm electron-vite build -m dev",
    "build:pro": "pnpm electron-vite build -m pro",
    "build:pro:mac": "npm run build:pro && electron-builder --mac",
    "build:pro:win": "npm run build:pro && electron-builder --win",
    "build:test": "pnpm electron-vite build -m test",
    "coverage": "vitest run --coverage",
    "deps:check": "pnpm outdated -r",
    "dev": "electron-vite -m dev",
    "dev:mock": "electron-vite -m dev-mock",
    "dev:mock-server": "pnpm mock-server -c mock.config.ts",
    "dev:mock:web": "vite -m dev-mock",
    "dev:pro": "electron-vite -m pro",
    "dev:test": "electron-vite -m test",
    "preinstall": "npx only-allow pnpm",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:style && pnpm run lint:format",
    "lint:eslint": "eslint --max-warnings 0  \"**/*.{ts,tsx,js,jsx,cjs,mjs}\" --fix",
    "lint:format": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,html,less,css,json}\"",
    "lint:style": "stylelint \"**/*.{css,less}\" --fix",
    "mock-gen": "pnpm http-mock-gen -c mock-gen.config.ts",
    "prepare": "husky",
    "preview:dev": "pnpm electron-vite preview -m dev",
    "preview:pro": "pnpm electron-vite preview -m pro",
    "preview:test": "pnpm electron-vite preview -m test",
    "test": "vitest run",
    "test-u": "vitest -u run",
    "typecheck": "tsc --noEmit",
    "update:deps": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "koffi": "^2.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@commitlint/prompt-cli": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@liangskyli/axios-request": "^0.2.2",
    "@liangskyli/http-mock-gen": "^7.0.1",
    "@liangskyli/mock": "^7.0.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.16",
    "antd": "^6.1.1",
    "axios": "^1.13.2",
    "classnames": "^2.5.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "immer": "^11.0.1",
    "jsdom": "^27.3.0",
    "less": "^4.5.1",
    "lint-staged": "^16.2.7",
    "optics-ts": "^2.4.1",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^19.2.3",
    "react-dev-inspector": "^2.0.1",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "stylelint": "^16.26.1",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.16",
    "zustand": "^5.0.9"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": "^20.19.0 || >=22.12.0",
    "pnpm": ">=10"
  }
}
